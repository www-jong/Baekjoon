    "n=int(input());a=0;i=0\n",
    "while n>0:a+=1;n-=a;i+=1; \n",
    "if i%2==0:\n",
    "    print(\"%d/%d\"%((i+n),(1-n)))\n",
    "else:\n",
    "    print(\"%d/%d\"%((1-n),(i+n)))"
