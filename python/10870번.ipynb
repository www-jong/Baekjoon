   "source": [
    "n=int(input())\n",
    "p=[0,1]\n",
    "if n>=2:\n",
    "    for i in range(2,n+1):\n",
    "        p.append(p[i-1]+p[i-2])\n",
    "    print(p[n])\n",
    "else:\n",
    "    print(n)\n",
    "\n"
   ]
